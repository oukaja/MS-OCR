docker pull ubuntu:latest
docker run -it ubuntu:latest /bin/bash # docker run -it -p 5000 ubuntu:latest /bin/bash
apt -y upgrade && apt -y update
apt -y install python3.7 python3-pip tesseract-ocr wget git libsm6 libxext6 libxrender-dev
cd home
